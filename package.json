{
  "name": "heyhon",
  "version": "1.0.0",
  "description": "Landing page for HeyHon",
  "main": "index.html",
  "scripts": {
    "watch:sass": "node-sass src/scss/main.scss docs/css/style.css -w",
    "dev": "webpack-dev-server --mode development --open",
    "start": "npm-run-all --parallel dev watch:sass",
    "compile:sass": "node-sass src/sass/main.scss docs/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' docs/css/style.comp.css -o docs/css/style.prefix.css",
    "compress:css": "node-sass docs/css/style.prefix.css docs/css/style.css --output-style compressed",
    "build:js": "webpack --mode production",
    "build": "npm-run-all build:js compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lisasiva/heyhon.git"
  },
  "author": "Lisa Siva",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lisasiva/heyhon/issues"
  },
  "homepage": "https://github.com/lisasiva/heyhon#readme",
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "babel-loader": "^8.0.6",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "dependencies": {
    "autoprefixer": "^9.6.4",
    "babel-polyfill": "^6.26.0",
    "live-server": "^1.2.1",
    "postcss-cli": "^6.1.3"
  }
}
